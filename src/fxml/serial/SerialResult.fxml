                                    translateX="-2178.0" translateY="-189.0" visible="false"/>
                        <fx:include fx:id="capacitor5" layoutX="244.0" layoutY="78.0" source="Capacitor.fxml"
                                    translateX="-2985.0" translateY="-189.0" visible="false"/>
                    </children>
                </HBox>

                <HBox fx:id="inductorControl" layoutX="-276.0" layoutY="78.0">
                    <children>
                        <fx:include fx:id="inductor1" layoutX="-276.0" layoutY="78.0" source="Inductor.fxml"
                                    translateX="243.0" translateY="-189.0" visible="false"/>
                        <fx:include fx:id="inductor2" layoutX="-146.0" layoutY="78.0" source="Inductor.fxml"
                                    translateX="-564.0" translateY="-189.0" visible="false"/>
