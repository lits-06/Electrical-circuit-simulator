                </HBox>

                <HBox fx:id="lineEndControl" layoutX="-276.0" layoutY="78.0">
                    <children>
                        <Line fx:id="setOnCount1" endX="125.0" rotate="90.0" strokeWidth="2.2" translateX="527.0"
                              translateY="115.5" visible="false"/>
                        <Line fx:id="setOnCount2" endX="125.0" rotate="90.0" strokeWidth="2.2" translateX="527.0"
                              translateY="115.5" visible="false"/>
                        <Line fx:id="setOnCount3" endX="125.0" rotate="90.0" strokeWidth="2.2" translateX="530.0"
                              translateY="115.5" visible="false"/>
